<%= form_with url: "/merchants/#{@merchant.id}/items", method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :description %>
  <%= form.text_field :description %>
  <%= form.label :unit_price %>
  <%= form.text_field :unit_price %>
  <%= form.submit 'Submit' %>
<% end %>